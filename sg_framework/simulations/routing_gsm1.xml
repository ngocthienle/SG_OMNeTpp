<config>
 <!-- gsm station -->
 <local node="gsmStation" routePackets="on" mobileIPv6Support="off">
  <interface name="ppp0" AdvSendAdvertisements="on" AdvHomeAgent="off">
   <inetAddr tentative="false">2002:7::0</inetAddr>
   <AdvPrefixList>
    <AdvPrefix AdvOnLinkFlag="on" AdvRtrAddrFlag="on" AdvValidLifetime="0" AdvPreferredLifetime="604800" AdvAutonomousFlag="true">2002::/16</AdvPrefix>
